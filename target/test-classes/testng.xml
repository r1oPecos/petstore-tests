<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Petstore Store Suite" parallel="methods" thread-count="2">
    <listeners>
        <listener class-name="com.example.petstore.tests.utils.TestListener"/>
    </listeners>
    <test name="Store Tests">

        <groups>
            <run>
                <include name="Store"/>
            </run>
        </groups>
        <classes>
            <class name="com.example.petstore.tests.store.StoreTests"/>
        </classes>
    </test>
</suite>
